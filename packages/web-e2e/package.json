{
  "name": "@crater/web-e2e",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "echo \"No build step for Playwright tests\"",
    "test": "echo \"Skipping e2e in default test run. Use pnpm --filter @crater/web-e2e test:e2e\"",
    "test:e2e": "playwright test",
    "test:headed": "playwright test --headed",
    "test:e2e:headed": "pnpm test:headed",
    "test:debug": "playwright test --debug",
    "test:e2e:debug": "pnpm test:debug",
    "test:ui": "playwright test --ui",
    "test:e2e:ui": "pnpm test:ui",
    "playwright:install": "playwright install",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "clean": "rm -rf playwright-report test-results"
  },
  "devDependencies": {
    "@playwright/test": "^1.53.0",
    "@types/node": "^20.0.0",
    "crater-web": "workspace:*",
    "playwright": "^1.53.0",
    "typescript": "^5.6.3"
  }
}
